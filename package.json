{
  "name": "apiboost",
  "type": "module",
  "version": "1.0.1",
  "private": false,
  "description": "一个增强api文档的工具",
  "keywords": [
    "apiboost",
    "zpeak",
    "api",
    "boost",
    "openapi",
    "generate",
    "swagger"
  ],
  "authors": [
    {
      "name": "peak",
      "url": "https://zhanggaofeng.cn"
    }
  ],
  "homepage": "https://github.com/Iontheroad/apiboost",
  "repository": {
    "type": "git",
    "url": "https://github.com/Iontheroad/apiboost.git"
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "bin": {
    "apiboost": "/dist/bin/generate.js"
  },
  "scripts": {
    "build": "tsc",
    "test": "tsx bin/generate.ts",
    "generate": "node dist/bin/generate.js",
    "g": "node dist/bin/generate.js",
    "release": "npx bumpp --excute=\"pnpm i && pnpm build\"",
    "pb": "pnpm publish"
  },
  "devDependencies": {
    "@types/node": "^24.9.1",
    "@zpeak/openapi-adapter": "^1.0.9",
    "fs-extra": "^11.3.2",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.6",
    "typescript": "^5.6.3"
  },
  "dependencies": {
    "@types/fs-extra": "^11.0.4"
  }
}